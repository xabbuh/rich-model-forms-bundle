{
    "name": "sensiolabs-de/rich-model-forms-bundle",
    "description": "Provides additional data mapper options that ease the use of the Symfony Form component with rich models.",
    "keywords": ["Symfony", "form", "forms", "bundle", "rich model", "DDD", "domain-driven design"],
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Christian Flothmann",
            "email": "christian.flothmann@sensiolabs.de"
        },
        {
            "name": "Christopher Hertel",
            "email": "christopher.hertel@sensiolabs.de"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "infection/infection": "^0.12.2",
        "psr/container": "^1.0",
        "symfony/config": "^3.4||^4.0",
        "symfony/dependency-injection": "^3.4||^4.0",
        "symfony/form": "^3.4.15||^4.1.4",
        "symfony/framework-bundle": "^3.4||^4.0",
        "symfony/http-kernel": "^3.4||^4.0",
        "symfony/options-resolver": "^3.4.4||^4.0.4",
        "symfony/property-access": "^3.4.15||^4.1.4"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.13",
        "phpstan/phpstan": "^0.10",
        "phpunit/phpunit": "^7.0",
        "symfony/phpunit-bridge": "^3.4.19||^4.1.8",
        "symfony/translation": "^3.4||^4.0"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "SensioLabs\\RichModelForms\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SensioLabs\\RichModelForms\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.2-dev"
        }
    }
}
